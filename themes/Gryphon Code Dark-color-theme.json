			"settings": {
				"fontStyle": "italic",
				"foreground": "#cfdce9bb"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#cfdce9"
			}
		},
		{
			"name": "Colors",
